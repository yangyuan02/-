{
    "background": {
       "persistent": true,
       "scripts": [ "background.static.js" ]
    },
    "content_scripts": [ {
       "all_frames": true,
       "js": [ "content.static.js" ],
       "match_about_blank": true,
       "matches": [ "http://*/*", "https://*/*", "ftp://*/*", "file://*/*" ],
       "run_at": "document_start"
    } ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "description": "影刀Chrome自动化插件",
    "icons": {
       "128": "icons/shadow128.png",
       "16": "icons/shadow16.png",
       "32": "icons/shadow32.png",
       "48": "icons/shadow64.png"
    },
    "key": "",
    "manifest_version": 2,
    "name": "影刀",
    "permissions": [ "cookies", "management", "tabs", "debugger", "nativeMessaging", "downloads", "webNavigation", "clipboardRead", "clipboardWrite", "\u003Call_urls>" ],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "1.1"
 }
 